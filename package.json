    "bootstrap-sass": "^3.2.0",
    "font-awesome": "^4.1.0",
    "jquery": "^3.4.1",
    "string": "^1.9.0",
    "underscore": "^1.6.0"
  },
  "devDependencies": {
    "browserify": "~3.36.0",
    "browserify-shim": "^3.6.0",
    "coffeeify": "~0.6.0",
